<!DOCTYPE html>
<html lang="en">

<!--HEAD-->
<%- include('./partials/head') %>

<body>

    <main class="container">

        <section class="userListCrud">
            <h1 class="crudList__title">Registro de usuarios</h1>
            <form id="formRegister" class="crudList__form">
                <input type="text" placeholder="Name" id="nameInput" class="form__input">
                <input type="email" placeholder="Email" id="emailInput" class="form__input">
                <button type="submit" id="submitButton" class="button button--primary">Agregar</button>
            </form>
            <table class="crudList__table">
                <thead>
                    <tr>
                        <th class="table__header">Nombre</th>
                        <th class="table__header">Email</th>
                        <th class="table__header">Action</th>
                    </tr>
                </thead>
                <tbody id="tableBody" class="table__body">
                    
                </tbody>
            </table>
        </section>
    </main>

</body>
</html>