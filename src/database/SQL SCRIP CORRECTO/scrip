-- Create cursos table
CREATE TABLE `product` (
  `sku` int(20) UNSIGNED NOT NULL,
  `name` varchar(150) NOT NULL,
  `description` text NOT NULL, 
  `image` varchar(200) NOT NULL,
  `price` int(10) UNSIGNED NOT NULL, 
  `name_teacher` varchar(100) NOT NULL,
  PRIMARY KEY (`sku`)
);

