-- Create usuarios table
CREATE TABLE `user`(
  `sku` int(50) UNSIGNED NOT NULL,
  `name` varchar(150) NOT NULL,
  `apellido` varchar(150) NOT NULL, 
  `mail` varchar(200) NOT NULL,
  `contraseña` varchar(100) NOT NULL, 
  `categoria_sku` varchar(100) NOT NULL,
  PRIMARY KEY (`sku`)
);

